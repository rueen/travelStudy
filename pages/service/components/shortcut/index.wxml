<!--pages/service/components/shortcut/index.wxml-->
<scroll-view scroll-x="true">
  <view class="container" style="width: {{180*5 + 'rpx'}};">
    <view class="item" wx:for="{{list}}" wx:key="id" bindtap="bindtapItem" data-item="{{item}}">
        <icon name="service" size="56rpx" color="#333" />
        <text class="itemText">{{item.title}}</text>
    </view>
  </view>
</scroll-view>
